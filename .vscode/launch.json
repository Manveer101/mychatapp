{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Django Server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "8000"
      ],
      "django": true
    },
    {
      "name": "Create Superuser",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "createsuperuser"
      ],
      "django": true
    }
  ]
}
